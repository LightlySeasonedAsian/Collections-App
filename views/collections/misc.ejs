<% layout('layouts/boilerplate') %>
<h1>Your Misc. List</h1>
    <div>
        <a href="/">Home</a>
    </div>
        <% for (let misc of list){ %>
         <div class="card" mb-3>
            <div class="row">
                <div class="col-md-4">
                    <img class="img-fluid" alt="" src="<%=misc.image%>">
                </div>
                <div class="col-md-8">
                    <div class="card-body">
                        <h5 class="card-title"><%= misc.title %></h5>
                        <p class="card-text"><%= misc.description%></p>
                        <p class="card-text">
                            <small class="text-muted"><%=misc.author%></small>
                        </p>
                        <a class="btn btn-primary" href="/misc/<%=misc._id%>">View <%=misc.title%></a>

                    </div>
                </div>
            </div>
        </div>  
         <% } %>  
